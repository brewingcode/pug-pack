{
  "name": "pug-pack",
  "version": "1.3.0",
  "description": "Pseudo-webpack for developers who like clean formatting",
  "scripts": {
    "start": "nodemon -w src -w lib -w test -e pug,js,coffee,styl,css,svg,json,yml -x 'npm run build'",
    "build": "coffee -r ./lib/build -e 'build.verbose = true; build.self true'",
    "prod": "NODE_ENV=production npm run build"
  },
  "bin": {
    "pug-pack": "lib/cli.coffee"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "browser-sync": "^2.26.5",
    "coffeescript": "^2.3.2",
    "csso": "^3.5.1",
    "html-minifier": "^3.5.21",
    "js-yaml": "^3.12.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.3",
    "stylus": "^0.54.5",
    "svgo": "^1.1.1",
    "uglify-es": "^3.3.9"
  },
  "main": "lib/build.coffee",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/brewingcode/pug-pack"
}
